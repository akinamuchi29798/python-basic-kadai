      "source": [
        "def calculate_total_price(price, tax_rate):\n",
        "  tax_amount = price * (tax_rate / 100)\n",
        "  total_price = price + tax_amount\n",
        "  return int(total_price)\n",
        "\n",
        "price = 110\n",
        "tax_rate = 10\n",
        "\n",
        "total_price = calculate_total_price(price, tax_rate)\n",
        "print(f\"商品の合計金額は {total_price} 円です。\")"
      ]
    }
